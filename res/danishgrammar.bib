	keywords = {Voice of Competence, Devitt, I-language, non-conceptual content, representation},
}

@incollection{gross_linguistic_2020,
	address = {Oxford},
	title = {Linguistic intuitions: {Error} signals and the {Voice} of {Competence}},
	isbn = {978-0-19-884055-8},
	shorttitle = {Linguistic intuitions},
	url = {https://oxford.universitypressscholarship.com/10.1093/oso/9780198840558.001.0001/oso-9780198840558-chapter-2},
	abstract = {Linguistic intuitions are a central source of evidence across a variety of linguistic domains. They have also long been a source of controversy. This chapter aims to illuminate the etiology and evidential status of at least some linguistic intuitions by relating them to error signals of the sort posited by accounts of online monitoring of speech production and comprehension. The suggestion is framed as a novel reply to Michael Devitt’s claim that linguistic intuitions are theory-laden “central systems” responses rather than endorsed outputs of a modularized language faculty (the “Voice of Competence”). Along the way, it is argued that linguistic intuitions may not constitute a natural kind with a common etiology and that, for a range of cases, the process by which the intuitions used in linguistics are generated amounts to little more than comprehension.},
	language = {eng},
	urldate = {2021-08-15},
	booktitle = {Linguistic {Intuitions}},
	publisher = {Oxford University Press},
	author = {Gross, Steven},
	year = {2020},

@article{davidsen-nielsen_feature_1978,
	title = {The feature “gravity” in old {English} and {Danish} phonology},
	volume = {16},
	issn = {0374-0463},
	url = {https://doi.org/10.1080/03740463.1978.10412238},
	doi = {10.1080/03740463.1978.10412238},
	abstract = {In The Sound Pattern of English (Chomsky \&Halle 1968, p. 303, p. 306-308) the Jakobsonian feature “gravity”, defined acoustically as “predominance of the low (vs. high) part of the spectrum” (Jakobson \&Halle 1968, p. 431), 1 is discarded along with “compactness”, “diffuseness” and replaced by the articulatorily defined features “anterior”, “coronal”, “high”, “low”, and “back”, and some arguments in favour of this revision are proposed. However well-founded the introduction of Chomsky \&Halle's new features may be, phonologists are beginning to realize that “gravity” should be retained in the set of universal phonetic features. Thus Ladefoged draws attention to the historical change [x] {\textgreater} [f] in English words like rough, cough (1971, p. 44), where the development of a velar into a labial rather than a dental voiceless fricative is not understandable from an articulatory point of view, and on the basis of examples of this type he includes “gravity” in his universal phonetic framework as one of the crosscutting acoustic-auditory features which are needed (other such features being “sibilance” and “sonorant”). The same sound change is found in Middle Polish (cf. Andersen 1974, p. 37), where in the Spisz dialect original [x], when not syllable initial before a vowel, has developed into [f], cf. examples like [tfuš] ‘polecat’, [płafta] ‘sheet’, [gruf] ‘peas’ ({\textless} Old Polish [txoř, płaxta, grox] tchórz, pláchtá, groch). Similarly in western Old Saxon (before [t]) : genuft- (= genuht-) ‘sufficient’, wurftio (= wurhtio) ‘worker’, druftin (= druhtin) ‘lord’. The opposite development, [f] {\textgreater} [x], is found (also before [t]) in many German dialects, e.g. Middle Franconian nihte (Upper German niftel) ‘niece’, sachte (Upper German sanft) ‘soft’, and especially in Low German, thus already in Old Saxon: kraht ‘power’, ahter ‘after’, ohto ‘often’. In Castilian Spanish and Gascon, likewise, initial [f] developed Into [x] (except before [w] and [r]), 2 cf. examples like Castilian hierro, hablar, hondo and Gascon hèt, haría ({\textless} ferru, fabulare, fundo, factu, farina).},
	number = {2},
	urldate = {2019-08-19},
	journal = {Acta Linguistica Hafniensia},
	author = {Davidsen-Nielsen, Niels and Ørum, Henning},
	month = jul,
	year = {1978},
	pages = {201--213},
}


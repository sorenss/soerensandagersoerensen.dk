@article{emmertsen_realization_2010,
	title = {Realization as a {Device} for {Remedying} {Problems} of {Affiliation} in {Interaction}},
	volume = {43},
	url = {http://www.tandfonline.com/doi/abs/10.1080/08351811003738059#.VjpY6a6rRTY},
	doi = {10.1080/08351811003738059},
	abstract = {This article investigates the use of the Danish response token nåja in naturally occurring conversation. The article shows that nåja functions as a change-of-state token with which a speaker claims to have just now realized matters relevant for the prior talk. Whether caused by the speaker's recollection, recognition, or understanding of relevant matters, the realization token functions as a claim that (a) there were problems in the speaker's prior turns due to her lack of understanding, and (b) that these problems are now solved due to a change in the speaker's epistemic access. The realization token can therefore be used to remedy prior turns that are potentially or actually disaffiliative in a way that minimally involves questions of character or relationship.},
	language = {English},
	number = {2},
	journal = {Research on Language \& Social Interaction},
	author = {Emmertsen, Sofie and Heinemann, Trine},
	year = {2010},
	keywords = {Language, Interpersonal communication, Social interaction, EMCA, Epistemics, Danish, Conversation Analysis, Affiliation, Non-response, Response Tokens},
	pages = {109--132},
}


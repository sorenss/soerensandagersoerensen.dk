@incollection{heinemann_justifying_2018,
	title = {Justifying departures from progressivity: {The} {Danish} turn-initial particle \textit{altså}},
	abstract = {This chapter investigates the use of the Danish particle alts{\textbackslash}a a in turn-initial position. Turn-initial alts{\textbackslash}a a can be employed for prefacing a wide range of actions, including self- and other-initiated repair, questions, second stories and answers to both yes/no and wh-questions. We show that across these actions, participants in interaction produce alts{\textbackslash}a a to indicate (1) that the action they will produce departs from progressivity, (2) that it will expand on something prior, (3) that the departure is, therefore, justified, and (4) that it will contribute to reinstalling the progression of the larger on-going activity. Some of the actions that alts{\textbackslash}a a prefaces can also be prefaced by phrases like ‘you know’ or ‘I mean’, which seem to do at least some of the work that alts{\textbackslash}a a does, but alts{\textbackslash}a a is used more frequently and across a wider range of actions. In our discussion, we raise the possibility that the usefulness of alts{\textbackslash}a a is due to the fact that it allows its producer to indicate that no one party in the interaction was accountable or at fault for the departure.},
	language = {English},
	booktitle = {Between {Turn} and {Sequence}.},
	publisher = {John Benjamins Publishing Company},
	author = {Heinemann, Trine and Steensig, Jakob},
	editor = {Sorjonen, Marja-Leena and Heritage, John},
	year = {2018},
	keywords = {Grammar, Danish, Conversation Analysis, Questioning, Second stories, Repair, Progressivity, turn-initial particle},
	pages = {435--464},
}


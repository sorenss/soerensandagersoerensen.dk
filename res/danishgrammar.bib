@article{worsoe_questioning_2020,
	title = {Questioning questions in psychotherapeutic practice: {The} dialogical dynamics of change in therapy through clients questioning therapists},
	volume = {11},
	copyright = {Copyright (c) 2020},
	issn = {1904-7843},
	shorttitle = {Questioning questions in psychotherapeutic practice},
	url = {https://tidsskrift.dk/sss/article/view/121371},
	doi = {10.7146/sss.v11i1.121371},
	abstract = {The focus of this study is a particular type of questioning in psychotherapy: The unusual, yet recurrent, phenomenon of clients asking questions or making requests to the therapist and the way this alters the dialogical dynamics and therapeutic alliance between the two. Thus, we investigate how these types of question-answer cycles challenge the balance of the dialogical system of therapy including the normally accepted asymmetrical power relation between therapist and client. The analysis is informed by an ecological perspective which views the dialogical collaboration of therapist and client as forming a distributed cognitive system. The study shows how disaffiliation to questioning cycles on one hand stress the dialogical system through changing the language game, yet on the other hand, also entertain a subtle form of cooperativeness. The questioning cycles inform the dyadic system of therapist and client so that precautions can be made in order to secure the therapeutic alliance.},
	language = {en},
	number = {1},
	urldate = {2020-08-04},
	journal = {Scandinavian Studies in Language},
	author = {Worsøe, Line Brink and Jensen, Thomas Wiben},
	month = jul,
	year = {2020},
	note = {Number: 1},
	keywords = {Psychotherapy, Questioning, Interaction, Collaboration, Therapeutic alliance, Dis-alignment, Ecological and distributed cognition},
	pages = {279--317},
}

@article{mortensen_question_2020,
	title = {A question of control? {Forms} and functions of courtroom questioning in two different adversarial trial systems},
	volume = {11},
	copyright = {Copyright (c) 2020},
	issn = {1904-7843},
	shorttitle = {A question of control?},
	url = {https://tidsskrift.dk/sss/article/view/121370},
	doi = {10.7146/sss.v11i1.121370},
	abstract = {This paper compares the questioning of witnesses and defendants in American and Danish courtroom interaction on the basis of one American and three Danish criminal trials. A total of 780 questions are analysed in terms of their morphosyntactic properties as well as speech act functions. Following a general discussion of courtroom questioning and the notions of coercion and control, as well as an outline of legal cultural differences and similarities between American and Danish courtroom interaction, a coding system is developed for the linguistic comparison, and initial quantitative results of the comparison are discussed. Particular attention is given to declarative questions and the ‘communicative’ speech act function, as the linguistic and interactional features of these are shown to be explored in quite different ways in accordance with the legal and cultural contexts in which the courtroom questioning takes place.},
	language = {en},
	number = {1},
	urldate = {2020-08-04},
	journal = {Scandinavian Studies in Language},
	author = {Mortensen, Sune Sønderberg},
	month = jul,
	year = {2020},
	note = {Number: 1},
	keywords = {Pragmatics, Questioning, Control, Courtroom interaction, Morphosyntax, Coercion, Legal cultures},
	pages = {239--278},
}

@article{graf_how_2020,
	title = {How to investigate the local and global change potential of questioning sequences in executive coaching? {A} call for interdisciplinary research},
	volume = {11},
	copyright = {Copyright (c) 2020},
	issn = {1904-7843},
	shorttitle = {How to investigate the local and global change potential of questioning sequences in executive coaching?},
	url = {https://tidsskrift.dk/sss/article/view/121368},
	doi = {10.7146/sss.v11i1.121368},
	abstract = {Coaching outcome research convincingly argues that coaching is effective and facilitates  change in clients. While coaching practice literature depicts questions as key vehicle for such change, empirical findings as regards the local and global change potential of questions are so far largely missing in both (psychological) outcome research and (linguistic and psychological) process research on coaching. The local change potential of questions refers to a turn-by-turn transformation as a result of their sequentiality, the global change potential is related to the power of questions to initiate, process and finalize established phases of change. This programmatic article on questions, or rather questioning sequences, in executive coaching pursues two goals: firstly, it takes stock of available insights into questions in coaching and advocates for Conversation Analysis as a fruitful methodological framework to assess the local change potential of questioning sequences. Secondly, it points to the limitations of a local turn-by-turn approach to unravel the overall change potential of questions and calls for an interdisciplinary approach to bring both local and global effectiveness into relation. Such an approach is premised on conversational sequentiality and psychological theories of change and facilitates research on questioning sequences as both local and global agents of change across the continuum of coaching sessions. We present the TSPP Model as a first result of such an interdisciplinary cooperation.},
	language = {en},
	number = {1},
	urldate = {2020-08-04},
	journal = {Scandinavian Studies in Language},
	author = {Graf, Eva-Maria and Dionne, Frédérick and Spranz-Fogasy, Thomas},
	month = jul,
	year = {2020},
	note = {Number: 1},
	keywords = {Conversation Analysis, Interdisciplinarity, Coaching, Local and global effectiveness, Questioning sequences, TSPP Model},
	pages = {214--238},
}

@article{archer_understanding_2020,

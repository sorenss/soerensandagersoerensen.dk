}

@incollection{collins_semantic_2020,
	address = {Oxford},
	title = {Semantic and syntactic intuitions: {Two} sides of the same coin},
	isbn = {978-0-19-884055-8},
	shorttitle = {Semantic and syntactic intuitions},
	url = {https://oxford.universitypressscholarship.com/10.1093/oso/9780198840558.001.0001/oso-9780198840558-chapter-6},
	abstract = {The chapter argues that so-called semantic and syntactic intuitions, as appealed to as evidence in linguistic theory, are two sides of the same coin. Speaker-hearers do not have direct intuitive access to linguistic facts, but instead have intuitions about what can be said with a sentence; it is the theorist’s task to sieve such coarse-grained reports to discover the contribution of semantic and syntactic principles to the content associated with a sentence. The chapter contends that this conception better accommodates the complexity of linguistic phenomena vis-à-vis intuition. It also shows how the position is defensible against two troubling linguistic phenomena: one where we find interpretation without grammaticality, and another where we find grammaticality without interpretation. Both cases are illusory, at least as so described. In both, grammaticality (syntactic soundness) is aligned with semantic interpretability.},
	language = {eng},
	urldate = {2021-08-15},
	booktitle = {Linguistic {Intuitions}},
	publisher = {Oxford University Press},
	author = {Collins, John},
	year = {2020},
	doi = {10.1093/oso/9780198840558.003.0006},
	keywords = {evidence, semantic intuitions, syntactic intuitions, vacuous quantification},
}

@incollection{brocker_generative_2020,

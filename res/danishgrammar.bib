@incollection{sorensen_turfinalt_2015,
	address = {Aarhus},
	title = {Turfinalt \textit{eller} i spørgsmål i dansk interaktion},
	copyright = {All rights reserved},
	isbn = {978-87-91134-01-2},
	url = {http://pure.au.dk/portal/files/108214433/S_rensen_2015_Turfinalt_eller_i_sp_rgsm_l_i_dansk_interaktion.pdf},
	abstract = {P{\textbackslash}a a dansk er der stor forskel p{\textbackslash}a a skriftsproget og talesproget. I DanTIN-gruppen (Danish Talk-in-Interaction) arbejder forskere og studerende p{\textbackslash}a a en systematisk beskrivelse af det danske samtalesprog, som løbende gøres tilg{\textbackslash}a engelig via hjemmesiden Samtalegrammatik.dk. I dette opl{\textbackslash}a eg pr{\textbackslash}a esenteres et nyt konversationsanalytisk studie af ytringsfinalt "eller". Denne type af "eller" findes nemlig ikke beskrevet i traditionel skriftsprogsgrammatik, men forekommer ikke sj{\textbackslash}a eldent i naturlige samtaledata. Dette ytringsfinale "eller" er fundamentalt anderledes end konjunktionen "eller", idet der ikke forekommer noget element efter "eller". Jeg vil vise hvordan "eller" i stedet kan behandles som en partikel, der bruges i slutningen af en bestemt type ytringer.Mit datagrundlag er baseret p{\textbackslash}a a optagelser af naturligt forekommende samtaler p{\textbackslash}a a dansk, og dette giver mulighed for at vise at disse eller-markerede ytringer behandles af samtaleparterne som fuldt grammatiske ytringer. Udover selve beskrivelsen af den type ytringer, som "eller" kan forekomme i slutningen af, bidrager konversationsanalysen til at karakterisere det svar, som den n{\textbackslash}a este taler i interaktionen producerer. Forholdet mellem den eller-markerede ytring og dets svar p{\textbackslash}a avirkes endda af forholdet mellem de interagerende personer - og den overordnede strukturelle sammenh{\textbackslash}a eng i samtaler.},
	language = {Dansk},
	booktitle = {15. {Møde} om {Udforskningen} af {Dansk} {Sprog}},
	publisher = {Nordisk - Institut for Kommunikation og Kultur},
	author = {Sørensen, Søren Sandager},
	editor = {Hansen, Inger Schoonderbeek and Hougaard, Tina Thode},
	year = {2015},
	pages = {377--398},
}

